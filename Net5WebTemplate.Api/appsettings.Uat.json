{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EmailConfiguration": {
    "Email": "test@email.com",
    "Host": "localhost",
    "Port": 25,
    "Password": "Overridden by User Secrets or Environment Variables",
    "EnableSsl": true
  },
  "IdentityOptionsConfig": {
    "RequiredDigit": true,
    "RequiredLength": 10,
    "RequireLowercase": true,
    "RequiredUniqueChars": 3,
    "RequireUppercase": true,
    "MaxFailedAttempts": 3,
    "LockoutTimeSpanInDays": 1
  },
  "PasswordResetToken": {
    "LifespanInMinutes": 30
  },
  "ConnectionStrings": {
    "Net5WebTemplateDbConnection": "server=(localdb)\\MSSQLLocalDB;Database=applicationdb;Trusted_Connection=true;"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/*",
        "Period": "1s",
        "Limit": 100
      }
    ]
  },
  "JwtSettings": {
    "Secret": "Overridden by User Secrets or Environment Variables",
    "Expiration": "00:05:00",
    "ValidateIssuerSigningKey": true,
    "ValidateIssuer": false,
    "ValidateAudience": false,
    "RequireExpirationTime": false,
    "ValidateLifetime": true
  }
}